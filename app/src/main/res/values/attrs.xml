<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">
    <declare-styleable name="IndicatorsView">
        <attr name="selectedDrawable" format="reference" />
        <attr name="unSelectedDrawable" format="reference" />
        <attr name="indicatorSize" format="dimension" />
        <attr name="paddingBetweenIndicators" format="dimension" />
        <attr name="numberOfIndicators" format="integer" />
        <attr name="selectedIndicator" format="integer" />

        <!-- ### -->
        <!-- <attr name="testColor" format="color"/>
        <attr name="testInteger" format="integer"/>
        <attr name="testReference" format="reference"/>
        <attr name="testDimension" format="dimension"/>
        <attr name="testBoolean" format="boolean"/>-->
    </declare-styleable>

    <declare-styleable name="TagFlowLayout">
        <attr name="max_select" format="integer"></attr>
        <attr name="tag_gravity">
            <enum name="left" value="-1" />
            <enum name="center" value="0" />
            <enum name="right" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="ExpandableLayout">
        <attr name="el_duration" format="integer" />
        <attr name="el_expanded" format="boolean" />
        <attr name="el_parallax" format="float" />
        <attr name="android:orientation" />
    </declare-styleable>


    <declare-styleable name="WaitingDots">
        <attr name="dotsColor" format="color" />
        <attr name="period" format="integer" />
        <attr name="jumpHeight" format="integer" />
        <attr name="autoplay" format="boolean" />
    </declare-styleable>


    <declare-styleable name="MaskedEditText">
        <attr name="mask" format="string" />
        <attr name="notMaskedSymbol" format="string" />
        <attr name="replacementChar" format="string" />
        <attr name="deleteChar" format="string" />
        <attr name="format" format="string" />
        <attr name="maskIcon" format="reference" />
        <attr name="maskIconColor" format="reference" />
        <attr name="android:inputType" />
    </declare-styleable>

    <declare-styleable name="MaskEditText">
        <attr name="met_mask" format="string" />
    </declare-styleable>

    <declare-styleable name="HorizontalPicker">
        <attr name="android:textColor" />
        <attr name="android:textSize" />
        <attr name="values" format="reference" />
        <attr name="android:ellipsize" />
        <attr name="android:marqueeRepeatLimit" />
        <attr name="dividerSize" format="dimension" />
        <attr name="sideItems" format="integer" />
    </declare-styleable>


    <attr name="horizontalPickerStyle" format="reference" />

    <!--<declare-styleable name="RangeSeekBar">
        <attr name="absoluteMinValue" format="integer|float"/>
        <attr name="absoluteMaxValue" format="integer|float"/>
        <attr name="singleThumb" format="boolean"/>
    </declare-styleable>-->

    <declare-styleable name="ShimmerLayout">
        <attr name="shimmer_color" format="color|reference" />
        <attr name="shimmer_animation_duration" format="integer|reference" />
        <attr name="shimmer_auto_start" format="boolean|reference" />
        <attr name="shimmer_angle" format="integer|reference" />
        <attr name="shimmer_mask_width" format="float|reference" />
        <attr name="shimmer_gradient_center_color_width" format="float|reference" />
        <attr name="shimmer_reverse_animation" format="boolean|reference" />
    </declare-styleable>

    <declare-styleable name="SlidingUpPanelLayout">
        <attr name="umanoPanelHeight" format="dimension" />
        <attr name="umanoShadowHeight" format="dimension" />
        <attr name="umanoParallaxOffset" format="dimension" />
        <attr name="umanoFadeColor" format="color" />
        <attr name="umanoFlingVelocity" format="integer" />
        <attr name="umanoDragView" format="reference" />
        <attr name="umanoScrollableView" format="reference" />
        <attr name="umanoOverlay" format="boolean" />
        <attr name="umanoClipPanel" format="boolean" />
        <attr name="umanoAnchorPoint" format="float" />
        <attr name="umanoInitialState" format="enum">
            <enum name="expanded" value="0" />
            <enum name="collapsed" value="1" />
            <enum name="anchored" value="2" />
            <enum name="hidden" value="3" />
        </attr>
        <attr name="umanoScrollInterpolator" format="reference" />
    </declare-styleable>

    <declare-styleable name="RealtimeBlurView" tools:ignore="MissingDefaultResource">
        <attr name="realtimeBlurRadius" format="dimension" />
        <attr name="realtimeDownsampleFactor" format="float" />
        <attr name="realtimeOverlayColor" format="color" />
    </declare-styleable>
    <declare-styleable name="OrderStatus">
        <attr name="processing" format="boolean"/>
        <attr name="completed" format="boolean"/>
    </declare-styleable>


    <declare-styleable name="LuckyWheel">

        <attr name="background_color" format="color"/>
        <attr name="arrow_image" format="reference"/>
        <attr name="square_layout" format="boolean"/>
        <attr name="image_padding" format="dimension"/>
    </declare-styleable>

    <!--new spinWheel-->
    <declare-styleable name="MyTextView">
        <attr name="font" format="reference" />
    </declare-styleable>

    <declare-styleable name="LuckyWheelView">
        <attr name="lkwBackgroundColor" format="color" />
        <attr name="lkwCursor" format="reference" />
        <attr name="lkwCenterImage" format="reference" />
        <attr name="lkwEdgeColor" format="color" />
        <attr name="lkwEdgeWidth" format="integer" />
        <attr name="lkwTopTextColor" format="color" />
        <attr name="lkwTopTextPadding" format="dimension" />
        <attr name="lkwTopTextSize" format="dimension" />
        <attr name="lkwSecondaryTextSize" format="dimension" />
    </declare-styleable>

    <declare-styleable name="WeekCalendar">
        <attr name="numOfPages" format="integer"/>
        <attr name="daysTextSize" format="dimension"/>
        <attr name="daysTextColor" format="color"/>
        <attr name="daysBackgroundColor" format="color"/>
        <attr name="weekTextSize" format="dimension"/>
        <attr name="weekTextColor" format="color"/>
        <attr name="weekBackgroundColor" format="color"/>
        <attr name="selectedBgColor" format="color"/>
        <attr name="todaysDateBgColor" format="color"/>
        <attr name="dayNameLength">
            <enum name="singleLetter" value="0"/>
            <enum name="threeLetters" value="1"/>
        </attr>
        <attr name="hideNames"  format="boolean"/>

        <!-- danigarciaalva -->
        <attr name="todaysDateTextColor" format="color"/>
    </declare-styleable>



    <!--Ruler-->

    <attr name="min_value" format="integer" />
    <attr name="max_value" format="integer" />

    <attr name="long_height_height_ratio" format="fraction" />
    <attr name="short_height_height_ratio" format="fraction" />

    <attr name="ruler_text_color" format="color" />
    <attr name="ruler_text_size" format="dimension" />

    <attr name="indicator_interval" format="dimension" />
    <attr name="indicator_color" format="color" />
    <attr name="indicator_width" format="dimension" />

    <attr name="notch_color" format="color" />

    <declare-styleable name="RulerView">
        <attr name="min_value" />
        <attr name="max_value" />

        <attr name="long_height_height_ratio" />
        <attr name="short_height_height_ratio" />

        <attr name="ruler_text_color" />
        <attr name="ruler_text_size" />

        <attr name="indicator_interval" />
        <attr name="indicator_color" />
        <attr name="indicator_width" />
    </declare-styleable>

    <declare-styleable name="RulerValuePicker">

        <attr name="min_value" />
        <attr name="max_value" />

        <attr name="long_height_height_ratio" />
        <attr name="short_height_height_ratio" />

        <attr name="ruler_text_color" />
        <attr name="ruler_text_size" />

        <attr name="indicator_interval" />
        <attr name="indicator_color" />
        <attr name="indicator_width" />

        <!-- Color of the top notch -->
        <attr name="notch_color" />
    </declare-styleable>
</resources>